<% @another_entries.each do |entry| %>
  <%= link_to room_path(entry.room) do %>
    <div class="card">
      <div class="card-body">
         <div>
           <div class="d-flex">
             <div class="chat-avatar">
                <object>
                  <%= link_to user_path(entry.user) do %>
                    <%= image_tag entry.user.get_image(50,50) %>
                  <div class="chat-user-name">
                    <%= entry.user.name %>
                  </div>
                  <div class="dm-text">
                    <%= Message.find_by(id: entry.room.message_ids.last)&.dm_body %>
                  </div>
                  <% end %>
                 </object>
             </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>
<% end %>